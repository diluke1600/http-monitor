monitor:
  urls:
    - "https://example.com"
    - "https://api.example.com/health"
  interval_seconds: 10      # 轮询间隔秒
  timeout_seconds: 5        # 单次请求超时时间秒

feishu:
  webhook: "https://open.feishu.cn/open-apis/bot/v2/hook/your-webhook"

log:
  file: "monitor.log"

alert:
  cooldown_seconds: 60        # 告警最小间隔，0 表示不限制
  latency_threshold_ms: 1000  # 超过该耗时判定为慢请求，0 表示关闭


